pipeline {
    agent any

    stages {
        stage('Setup') {
            steps {
                git branch: 'main', url: 'https://github.com/RodrigorMonteiro/teste-ebac-ui.git'
                bat 'npm install'
 bat 'npm install cypress --save-dev'
            }
        }
        stage('Test') {
            steps {
                bat '''set NO_COLOR=1
npm test'''
            }
        }
    }
}
